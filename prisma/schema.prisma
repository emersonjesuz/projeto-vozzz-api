datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int      @id @default(autoincrement())
  name     String
  email    String   @unique
  birth    DateTime
  password String
  actived  Boolean  @default(true)
  delete   Boolean  @default(false)
  // Perfil   Perfil[]
  // Post     Post[]

  @@map("users")
}

